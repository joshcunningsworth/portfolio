    {{{body}}}

    <script>
        for (var i = 1; i != 7; i++) {
            document.getElementById('nav' + i).addEventListener('click', smoothScroll);
        }

        function smoothScroll(element, to, duration = 600) {
            var element = document.body;
            if (this.id == 'nav1') {
                var to = document.getElementById('home').offsetTop;
            } else if (this.id == 'nav2') {
                var to = document.getElementById('cpp').offsetTop;
            } else if (this.id == 'nav3') {
                var to = document.getElementById('web').offsetTop;
            } else if (this.id == 'nav4') {
                var to = document.getElementById('java').offsetTop;
            } else if (this.id == 'nav5') {
                var to = document.getElementById('design').offsetTop;
            } else if (this.id == 'nav6') {
                var to = document.getElementById('sql').offsetTop;
            }
            if (duration <= 0) return;
            var difference = to - element.scrollTop;
            var perTick = difference / duration * 15;

            setTimeout(function () {
                element.scrollTop = element.scrollTop + perTick;
                if (element.scrollTop === to) return;
                smoothScroll(element, to, duration - 15);
            }, 15);
        }
    </script>
